{
  "name": "e2e-init",
  "private": "true",
  "version": "1.0.0",
  "description": "e2e init project",
  "main": "index.js",
  "scripts": {
    "test": "npm run cy:open",
    "cy:open": "cypress open --e2e",
    "cy:open:chrome": "cypress open --e2e --browser chrome",
    "cy:open:chrome:mobile": "cypress open --e2e --browser chrome --config viewportWidth=375,viewportHeight=667",
    "cy:run:chrome:mobile": "cypress run --config viewportWidth=375,viewportHeight=667",
    "cy:run": "cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e2e-testing-wg/e2e-init.git"
  },
  "keywords": [
    "testing",
    "e2e",
    "cypress"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/e2e-testing-wg/e2e-init/issues"
  },
  "homepage": "https://github.com/e2e-testing-wg/e2e-init#readme",
  "devDependencies": {
    "cypress": "^10.11.0",
    "cypress-mochawesome-reporter": "^3.2.3"
  }
}
